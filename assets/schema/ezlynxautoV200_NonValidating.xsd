<grammar
	xmlns:rng="http://relaxng.org/ns/structure/1.0"
	xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
	xmlns="http://www.ezlynx.com/XMLSchema/Auto/V200"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" ns="http://www.ezlynx.com/XMLSchema/Auto/V200" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
	<define name="int10">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">10</param>
		</data>
	</define>
	<define name="int50">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">50</param>
		</data>
	</define>
	<define name="int100">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">100</param>
		</data>
	</define>
	<define name="int255">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">255</param>
		</data>
	</define>
	<define name="int999">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">999</param>
		</data>
	</define>
	<define name="int100000">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">100000</param>
		</data>
	</define>
	<define name="int999999">
		<data type="nonNegativeInteger">
			<param name="minInclusive">0</param>
			<param name="maxInclusive">999999</param>
		</data>
	</define>
	<define name="NamePrefixType">
		<choice>
			<value>MR</value>
			<value>MRS</value>
			<value>MS</value>
			<value>DR</value>
		</choice>
	</define>
	<define name="NameSuffixType">
		<choice>
			<value>JR</value>
			<value>SR</value>
			<value>I</value>
			<value>II</value>
			<value>III</value>
		</choice>
	</define>
	<define name="NameType">
		<optional>
			<element name="Prefix">
				<ref name="NamePrefixType"/>
			</element>
		</optional>
		<element name="FirstName">
			<ref name="ProperName"/>
		</element>
		<element name="MiddleName">
			<ref name="MiddleInitial"/>
		</element>
		<element name="LastName">
			<ref name="ProperName"/>
		</element>
		<optional>
			<element name="Suffix">
				<ref name="NameSuffixType"/>
			</element>
		</optional>
	</define>
	<define name="SSNType">
		<data type="string">
			<param name="pattern">\d{9}</param>
		</data>
	</define>
	<define name="Zip5Type">
		<data type="string">
			<param name="pattern">\d{5}</param>
		</data>
	</define>
	<define name="Zip4Type">
		<data type="string">
			<param name="pattern">\d{4}</param>
		</data>
	</define>
	<define name="DateType">
		<data type="date"/>
	</define>
	<define name="YearType">
