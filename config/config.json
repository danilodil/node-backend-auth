{
  "local": {
    "use_env_variable": "DATABASE_URL_LOCAL",
    "dialect": "postgres"
  },
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres"
  },
  "staging": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres"
  },
  "production": {
      "use_env_variable": "DATABASE_URL",
      "ssl": true,
      "dialect": "postgres",
      "dialectOptions": {
        "ssl": {"require":true }
      }
  }
}